<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView 
		android:id="@+id/ry84ckek53p8"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:background="#FFFFFF">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/rjqd6nil7u1b"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#FFFFFF"
				android:paddingBottom="448dp"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/ro2cojcgcreq"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="#007AFF"
					android:paddingTop="42dp"
					android:paddingBottom="28dp"
					android:paddingLeft="26dp"
					android:paddingRight="12dp"
					android:layout_marginBottom="39dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/r7g5kdua7uaw"
						android:layout_width="59dp"
						android:layout_height="38dp"
						android:layout_marginEnd="2dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
					<TextView
						android:id="@+id/riqmp5azjim"
						android:textColor="#FFFFFF"
						android:textSize="40sp"
						android:textStyle="bold"
						android:layout_marginBottom="1dp"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:gravity="center"
						android:text="Centro de Ayuda"
					/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/r78qaf2go2i5"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="@drawable/cr28bece6f0"
					android:paddingTop="24dp"
					android:layout_marginBottom="42dp"
					android:layout_marginHorizontal="50dp"
					android:orientation="vertical">
					<TextView
						android:id="@+id/rzz1sq2yd5x9"
						android:textColor="#1D1B20"
						android:textSize="24sp"
						android:layout_marginBottom="16dp"
						android:layout_marginStart="24dp"
						android:layout_height="wrap_content"
						android:layout_width="224dp"
						android:text="Marque al siguiente númro para cualquier aclaración"
					/>
					<TextView
						android:id="@+id/requsnumm9ds"
						android:textColor="#49454F"
						android:textSize="14sp"
						android:layout_marginStart="24dp"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="XXX-XXX-XXXXX"
					/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/roc9q4445htp"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="@drawable/cr28bece6f0"
					android:paddingTop="24dp"
					android:layout_marginHorizontal="50dp"
					android:orientation="vertical">
					<TextView
						android:id="@+id/rto2mfu3dp2"
						android:textColor="#1D1B20"
						android:textSize="24sp"
						android:layout_marginBottom="16dp"
						android:layout_marginHorizontal="24dp"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:text="Descargue el manual de usuario."
					/>
					<TextView
						android:id="@+id/r6nz8qjizo8u"
						android:textColor="#49454F"
						android:textSize="14sp"
						android:layout_marginStart="24dp"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="Descargar aqui."
					/>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
