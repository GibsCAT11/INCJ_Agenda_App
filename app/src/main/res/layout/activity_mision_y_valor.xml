<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFF"
	android:clipChildren="false"
	android:clipToPadding="false"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView 
		android:clipChildren="false"
		android:clipToPadding="false"
		android:id="@+id/rbtj6e16ama"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:background="#FFFFFF">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:clipChildren="false"
			android:clipToPadding="false"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/rtf55wzmwj2"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#FFFFFF"
				android:paddingBottom="41dp"
				android:clipChildren="false"
				android:clipToPadding="false"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/r15ycfoxyshh"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="#007AFF"
					android:paddingTop="38dp"
					android:paddingBottom="28dp"
					android:paddingHorizontal="26dp"
					android:layout_marginBottom="39dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/rcaps9pvw8nv"
						android:layout_width="59dp"
						android:layout_height="38dp"
						android:layout_marginTop="4dp"
						android:layout_marginEnd="16dp"
						android:src="@mipmap/ic_launcher"
						android:scaleType="fitXY"
					/>
					<TextView
						android:id="@+id/rxkp312tn9bn"
						android:textColor="#FFFFFF"
						android:textSize="40sp"
						android:textStyle="bold"
						android:layout_height="wrap_content"
						android:layout_width="wrap_content"
						android:text="Misión Y Valor"
					/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/rfdtf5p5ritc"
					android:layout_width="match_parent"
					android:layout_height="166dp"
					android:background="@drawable/scac4d0sw1cr12bfef7ff"
					android:layout_marginBottom="23dp"
					android:layout_marginHorizontal="21dp"
					android:orientation="vertical">
				</LinearLayout>
				<LinearLayout
					android:id="@+id/r9wimfgy9e1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:clipChildren="false"
					android:clipToPadding="false"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/r6m586l2q339"
						android:layout_width="match_parent"
						android:layout_height="349dp"
						android:background="@drawable/scac4d0sw1cr12bfef7ff"
						android:layout_marginHorizontal="21dp"
						android:orientation="vertical">
					</LinearLayout>
					<TextView
						android:id="@+id/rqz30kp7qu1q"
						android:textColor="#49454F"
						android:layout_marginHorizontal="37dp"
						android:layout_height="wrap_content"
						android:layout_width="match_parent"
						android:layout_weight="1"
						android:text="Evangelización\n-Llevar la palabra de Cristo para las personas que no conozcan el cristianismo.\n-Darles la oportunidad de un Nuevo Comienzo.\nServicio a la comunidad\n-Atender las necesidades más profundas de las personas.\n-Transformar a la sociedad para que sea un lugar mejor y más pacifico.\nPromover la Fé\n-Perpetuar las enseñanzas de Jesucristo.\n-Preservar la palabra de Cristo obras y milagros.\n-Cultiva los atributos de Cristo en el hombre.\n-Representar a Cristo y sus valores de forma clara y precisa en el mundo.\n"
					/>
					<RelativeLayout
						android:id="@+id/rfpifq36yz2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:clipChildren="false"
						android:clipToPadding="false"
						>
						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:clipChildren="false"
							android:clipToPadding="false"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/rjz187d8ah1e"
								android:layout_width="match_parent"
								android:layout_height="179dp"
								android:src="@mipmap/ic_launcher"
								android:scaleType="fitXY"
							/>
						</LinearLayout>
						<RelativeLayout
							android:clipChildren="false"
							android:clipToPadding="false"
							android:id="@+id/rr8n22ik9yb_rl"
							android:layout_marginBottom="-6dp"
							android:layout_marginEnd="0dp"
							android:layout_marginStart="0dp"
							android:layout_alignParentBottom="true"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal"
							>
							<ImageView
								android:id="@+id/rr8n22ik9yb"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:src="@mipmap/ic_launcher"
								android:scaleType="fitXY"
							/>
							<LinearLayout
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:clipChildren="false"
								android:clipToPadding="false"
								android:orientation="vertical">
								<LinearLayout
									android:id="@+id/rudedz041org"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:gravity="center_vertical"
									android:background="@drawable/cr16bece6f0"
									android:padding="16dp"
									android:layout_marginTop="50dp"
									android:layout_marginBottom="53dp"
									android:elevation="3dp"
									android:clipChildren="false"
									android:clipToPadding="false"
									android:clickable="true"
									android:focusable="true"
									android:orientation="horizontal">
									<ImageView
										android:id="@+id/rho03gkclhbq"
										android:layout_width="24dp"
										android:layout_height="24dp"
										android:layout_marginEnd="12dp"
										android:src="@mipmap/ic_launcher"
										android:scaleType="fitXY"
									/>
									<TextView
										android:id="@+id/r4pdnsmh7lw6"
										android:textColor="#65558F"
										android:textSize="14sp"
										android:textStyle="bold"
										android:layout_height="wrap_content"
										android:layout_width="wrap_content"
										android:text="Editar"
									/>
								</LinearLayout>
							</LinearLayout>
						</RelativeLayout>
					</RelativeLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
